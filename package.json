{
  "name": "gform-trello-magic",
  "description": "Setup with puggle",
  "version": "0.4.0",
  "private": true,
  "repository": "unplatform/gform-to-trello",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "coverage": "jest --coverage --collectCoverageFrom='src/**/*.ts'",
    "cli:dev": "node -r dotenv/config -r ts-node/register src/cli.ts",
    "cli:prod": "node dist/cli.js",
    "lint": "eslint src",
    "prettier": "prettier --write '**/*.{js,json,css,md,ts,tsx}'",
    "preversion": "npm run test -s",
    "start": "npm run cli:prod ",
    "test": "jest",
    "generate:toc": "md-toc -i",
    "release": "standard-version"
  },
  "keywords": [],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@openlab/trello-client": "^0.1.1",
    "chalk": "^3.0.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "got": "^9.6.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "nunjucks": "^3.2.2",
    "prompts": "^2.3.0",
    "slugify": "^1.3.6",
    "superstruct": "^0.8.2",
    "valid-env": "^1.0.1",
    "yaml": "^1.7.2",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@google/clasp": "^2.3.0",
    "@robb_j/md-toc": "^0.3.1",
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/google-apps-script": "^1.0.6",
    "@types/got": "^9.6.9",
    "@types/jest": "^24.0.23",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/nunjucks": "^3.1.3",
    "@types/prompts": "^2.0.3",
    "@types/yaml": "^1.2.0",
    "@types/yargs": "^13.0.3",
    "inquirer": "^6.0.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.4",
    "nock": "^11.7.0",
    "prettier": "^1.16.4",
    "standard-version": "^8.0.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "gitHooks": {
    "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
